<h2>Create a New State</h2>
<% if @state.errors.any? %>
  <div id="error_explanation">
    <h2>
      <%= pluralize(@state.errors.count, "error") %>
      prohibited you from saving:
    </h2>

    <ul>
    <% @state.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>


<%= form_for @state do |f| %>

State: <%= f.text_field :name %> <br><br>

<%= f.submit  %>
<% end %>