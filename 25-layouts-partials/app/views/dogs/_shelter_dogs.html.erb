<table>
  <tr>
    <th>Name</th>
    <th>Breed</th>
    <th>Age</th>
    <th>Sex</th>
    <th>Shelter</th>
    <th>Adopt</th>
  </tr>
  <% dogs.adoptable.each do |dog|%>
    <tr>
      <td><strong><%= link_to dog.name, dog_path(dog) %></strong></td>
      <td><%=dog.breed%></td>
      <td><%= dog.age%></td>
      <td><%= display_gender(dog) %></td>
      <td><%= dog.shelter.name %></td>
      <td><button><%= link_to "Adopt Me!", "/dogs/#{dog.id}/adopt" %> </button> </td>
    </tr>
  <% end %>
</table>

